<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.splash">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo6"
        android:label="Restaurant Ordering System"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".add_food"></activity>
        <activity android:name=".forget" />
        <activity android:name=".bill" />
        <activity android:name=".detail" />
        <activity android:name=".order" />
        <activity android:name=".testing" />
        <activity android:name=".Menu" />
        <activity android:name=".Dashboard" />
        <activity android:name=".Signup" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>